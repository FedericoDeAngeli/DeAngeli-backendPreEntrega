<h1>Productos</h1>

<table id="table">
    <thead>
<tr>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Precio</th>
    <th>Thumbnail</th>
    <th>Code</th>
    <th>Categoria</th>
</tr>    
</thead>
<tbody id="tbody">
    {{#each productos}}
    <tr>

                        <td>{{this.title}}</td>
                        <td>{{this.description}}</td>
                          <td>{{this.price}}</td>
                          <td>{this.thumbnail}</td>
                          <td>{{this.code}}</td>                                     
                        <td>{{this.category}}</td>
</tr>
{{/each}}
</tbody>
</table>